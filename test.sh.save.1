#!/bin/bash

for i in bin/*.bin ; do
cp $i bin/input/input.bin; 


filename=$(basename $i .bin);
output=$(<$filename.txt);
echo $filename;
echo $output;

./prog
RETCODE=$?


if [[ "$RETCODE" == 246 ]] ; then

	echo "Success";
else
	echo "Failure";

fi

rm bin/input/input.bin;

echo $filename;
done

